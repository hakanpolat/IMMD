issn = {08858993},
journal = {IEEE Transactions on Power Electronics},
keywords = {Active energy buffer,GaN,flying capacitor multilevel (FCML),inverter,multilevel inverter,single phase},
number = {11},
pages = {8570--8581},
publisher = {IEEE},
title = {{A 2-kW Single-Phase Seven-Level Flying Capacitor Multilevel Inverter with an Active Energy Buffer}},
volume = {32},
year = {2017}
}
