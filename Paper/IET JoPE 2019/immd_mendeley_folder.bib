file = {:D$\backslash$:/mymendeley/Huang et al/Analytical loss model of high voltage GaN HEMT in cascode configuration/Analytical loss model of high voltage GaN HEMT in cascode configuration{\_}Huang et al.{\_}2014.pdf:pdf},
