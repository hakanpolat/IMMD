@article{Sun2019,
abstract = {Wide-bandgap devices have been widely used to reduce the size and increase the efficiency of power converters by operating at high switching frequency, at the expense of heightened conducted electromagnetic inference emissions (EMI), of which the latter circulate through the power loop and ancillary circuitry. In effect, the parasitic input-output capacitance Ci-o of the gate-driver power supply represents a key EMI propagation path to be controlled in order to ensure the operational integrity of power converters. To this end, this paper proposes an integrated, dual-output gate drive power supply for Gallium-Nitride (GaN) 650 V half-bridge phase-legs, rated at 2 W, featuring an ultra-low Ci-o of 1.6 pF, a power density of 72 W/in3, and an efficiency of 85 {\%}. All this is attained using an active-flyback converter switching at 1 MHz using GaN devices and a Pareto-optimized transformer design minimizing its interwinding capacitances, volume, and losses. The transformer is fully embedded in PCB material, doubling as substrate for the top-side active layer of the power supply. The paper presents the complete design procedure, processing, and experimental demonstration of the proposed power supply, evaluating as well the reliability impact of the magnetic-PCB material interface in high ambient temperature applications (200 {\&}{\#}x00B0;C). IEEE},
author = {Sun, Bingyao and Burgos, Rolando and Boroyevich, Dushan},
doi = {10.1109/TPEL.2018.2828384},
