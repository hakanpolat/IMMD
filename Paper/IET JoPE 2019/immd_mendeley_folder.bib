file = {:D$\backslash$:/mymendeley/Han et al/Comparative Analysis on Conducted CM EMI Emission of Motor Drives WBG Versus Si Devices/Comparative Analysis on Conducted CM EMI Emission of Motor Drives WBG Versus Si Devices{\_}Han et al.{\_}2017.pdf:pdf},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {Common mode (CM),Electromagnetic interference (EMI),Gallium nitride (GaN),Motor drive,Silicon carbide (SiC)},
number = {10},
pages = {8353--8363},
title = {{Comparative Analysis on Conducted CM EMI Emission of Motor Drives: WBG Versus Si Devices}},
volume = {64},
year = {2017}
}
