@article{McGrath2003,
abstract = {Previous work has shown that space vector modulation and carrier$\backslash$nmodulation for two-level inverters achieve the same phase leg switching$\backslash$nsequences when appropriate zero sequence offsets are added to the$\backslash$nreference waveforms for carrier modulation. This paper presents a$\backslash$nsimilar equivalence between the phase disposition carrier and space$\backslash$nvector modulation strategies applied to diode clamped, cascaded N-level$\backslash$nor hybrid multilevel inverters. By analysis of the time integral$\backslash$ntrajectory of the converter voltage, the paper shows that the optimal$\backslash$nharmonic profile for a space vector modulator occurs when the two middle$\backslash$nspace vectors are centred in each switching cycle. The required zero$\backslash$nsequence offset to achieve this centring for an equivalent carrier based$\backslash$nmodulator is then determined. The results can be applied to any$\backslash$nmultilevel converter topology without differentiation. Discontinuous$\backslash$nbehaviour is also examined, with the space vector and carrier based$\backslash$nmodulation methods shown to similarly produce identical performance.$\backslash$nBoth simulation and experimental results are presented},
author = {McGrath, Brendan Peter and Holmes, Donald Grahame and Lipo, Thomas},
