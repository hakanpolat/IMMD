keywords = {Design optimisation,electric machines,fault tolerance,permanent magnet machines,reluctance network,thermal network},
title = {{Evaluation of motor-drive segmentation strategies for fault-tolerance}},
year = {2013}
}
@inproceedings{Fabri2012,
abstract = {This paper refers to a DSP-based electronic control unit adopted to supervise the tracking position control of a double fault-tolerant multi-phase motor-drive for aircraft applications. Starting from the requirements of the system a multiprocessor architecture interconnected by means of high speed synchronous links is adopted to meet the system specifications. Hardware details concerning the adopted interfaces among sensors and controllers are discussed. Software management of the multiprocessor system are presented detailing design issues and implementation strategies. A prototype has been developed based on C2000 MCUs and experimental results are reported focusing on the data communications management.},
