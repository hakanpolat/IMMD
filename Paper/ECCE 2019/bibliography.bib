@inproceedings{Fu,
abstract = {An improved induction motor control strategy is proposed$\backslash$nincorporating a multiphase machine which allows for continuous,$\backslash$ndisturbance free operation of the drive even with complete loss of one$\backslash$n(or more) legs of the inverter or motor phase. A complete analysis of$\backslash$nboth even phase and odd phase machines utilizing this control and$\backslash$ncircuit concept is presented. By using a five-phase or a seven-phase$\backslash$nmachine, it is possible to take advantage of the additional degrees of$\backslash$nfreedom as a result of more phases. It is shown that when one phase is$\backslash$nopen circuited, with appropriate control, the zero sequence current is$\backslash$nno longer a necessary component as in a three-phase machine. That is,$\backslash$none can eliminate the need for a neutral line and the machine can still$\backslash$nbe controlled so as to produce a smooth nonpulsating torque. It has also$\backslash$nbeen determined that the ratings of motor and inverter will both benefit$\backslash$nby the use of extra phases},
author = {Fu, J.-R. and Lipo, T.A.},
booktitle = {Conference Record of the 1993 IEEE Industry Applications Conference Twenty-Eighth IAS Annual Meeting},
doi = {10.1109/IAS.1993.298884},
