year = {2007}
}
@article{Lin2013,
abstract = {An intelligent complementary sliding-mode control (CSMC)$\backslash$n(ICSMC) is proposed in this paper for the fault-tolerant control of a$\backslash$nsix-phase permanent-magnet synchronous motor (PMSM) drive system with$\backslash$nopen phases. First, the dynamics of the six-phase PMSM drive system with$\backslash$na lumped uncertainty is described in detail. Then, the fault detection$\backslash$nand operating decision method is briefly introduced. Moreover, a CSMC is$\backslash$ndesigned to stabilize the fault-tolerant control of the six-phase PMSM$\backslash$ndrive system. Furthermore, to improve the required control performance$\backslash$nand to maintain the stability of the six-phase PMSM drive system under$\backslash$nfaulty condition, the ICSMC is developed. In this approach, a$\backslash$nTakagi-Sugeno-Kang-type fuzzy neural network with asymmetric membership$\backslash$nfunction (TSKFNN-AMF) estimator with accurate approximation capability$\backslash$nis employed to estimate the lumped uncertainty. In addition, the$\backslash$nadaptive learning algorithms for the online training of the TSKFNN-AMF$\backslash$nare derived using the Lyapunov theorem to guarantee the closed-loop$\backslash$nstability. Additionally, to enhance the control performance of the$\backslash$nproposed intelligent fault-tolerant control, a 32-b floating-point$\backslash$ndigital signal processor, TMS320F28335, is adopted for the$\backslash$nimplementation of the proposed fault-tolerant control system. Finally,$\backslash$nsome experimental results are illustrated to demonstrate the validity of$\backslash$nthe proposed fault-tolerant control for the six-phase PMSM drive system$\backslash$nvia ICSMC.},
author = {Lin, Faa Jeng and Hung, Ying Chih and Tsai, Meng Ting},
doi = {10.1109/TIE.2013.2238877},
file = {::},
isbn = {0278-0046},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {Asymmetric membership function (MF) (AMF),Takagi-Sugeno-Kang (TSK)-type fuzzy neural network (NN) (FNN),complementary sliding-mode control (SMC) (CSMC),fault-tolerant control,six-phase permanent-magnet synchronous motor (PMSM)},
title = {{Fault-tolerant control for six-phase PMSM drive system via intelligent complementary sliding-mode control using TSKFNN-AMF}},
year = {2013}
}
@article{Stabile2015,
abstract = {Power converters are complex devices subject to failures that can lead to undesired stops. For this reason, many fault-tolerant topologies and strategies have been deeply studied, mainly focusing on improving the electromechanical behavior of the reconfigured drive after the fault. However, it is also very important to evaluate the possible increase of power losses. In this paper, a power loss analysis is performed on a current-controlled three-phase inverter supplying a permanent-magnet synchronous motor (just chosen as an example), for all the operating modes, starting from the fault occurrence until the faulty operation mode is established. This is particularly important because the effectiveness of a fault-tolerant control strategy depends on the possibility to maintain a continuous operation. The latter is ensured only if there is no such increment of the losses that the survival of the component is at risk. The simulations are validated by means of experimental tests, and the results are discussed. The method can be easily applied to other topologies and reconfiguration strategies.},
author = {Stabile, Antonino and Estima, Jorge O. and Boccaletti, Chiara and {Marques Cardoso}, Antonio J.},
doi = {10.1109/TIE.2014.2385652},
file = {::},
isbn = {0278-0046},
